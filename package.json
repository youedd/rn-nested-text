{
  "name": "rn-styled-nested-text",
  "version": "0.0.1",
  "source": "src/index.ts",
  "main": "dist/main.js",
  "module": "dist/module.js",
  "types": "dist/types.d.ts",
  "license": "MIT",
  "scripts": {
    "postinstall": "yarn build",
    "build": "yarn build:clean && yarn build:parser && yarn build:main",
    "build:clean": "del dist src/parser.js .parcel-cache",
    "build:parser": "jison src/parser/parser.jison -o src/parser/parser.js",
    "build:main": "parcel build",
    "lint": "tsc && ts-standard",
    "lint:fix": "ts-standard --fix",
    "test": "yarn lint && yarn jest"
  },
  "devDependencies": {
    "@parcel/packager-ts": "^2.1.0",
    "@parcel/transformer-typescript-types": "^2.1.0",
    "@types/jest": "^27.4.0",
    "@typescript-eslint/eslint-plugin": "^5.9.0",
    "@typescript-eslint/parser": "^5.9.0",
    "del-cli": "^4.0.1",
    "husky": "^7.0.4",
    "jest": "^27.4.7",
    "jison": "^0.4.18",
    "parcel": "^2.1.0",
    "ts-jest": "^27.1.2",
    "typescript": "^4.5.4",
    "ts-standard": "^11.0.0"
  },
  "husky": {
    "hooks": {
      "pre-push": "yarn test"
    }
  }
}
